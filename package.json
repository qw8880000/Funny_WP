{
  "name": "funny_wp",
  "version": "1.0.0",
  "description": "WordPress theme",
  "bugs": {
    "url": "https://github.com/qw8880000/funny-wp/issues"
  },
  "homepage": "https://github.com/qw8880000/funny-wp#readme",
  "scripts": {
    "build:style": "node-sass style.scss style.css --output-style expanded && postcss -r style.css",
    "build": "run-p \"build:*\"",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.5.0",
    "browserslist": "^4.6.4",
    "chokidar-cli": "^1.2.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "bourbon": "^5.0.0"
  }
}
